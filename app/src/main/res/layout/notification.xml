<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_baseline_notifications_none_24"
        tools:layout_editor_absoluteY="1168dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/NotifiIcon"
        android:layout_width="40dp"
        android:layout_height="34dp"
        android:layout_marginStart="352dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_notifications_none_24" />
    <TextView
        android:id="@+id/notifiText"
        android:layout_width="241dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:background="#0CE5ECE5"
        android:fontFamily="sans-serif-black"
        android:includeFontPadding="true"
        android:text="NOTFICATIONS"
        android:textAlignment="center"
        android:textColor="#007C80"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="47dp"
        android:layout_height="35dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toEndOf="@+id/notifiText"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_search_24" />
    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="26dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />

    <TextView
        android:id="@+id/notifi1"
        android:layout_width="407dp"
        android:layout_height="70dp"
        android:layout_marginTop="56dp"
        android:background="#007C80"
        android:text="   Welcome to Easy Works . This platform is for you "
        android:textColor="#EFE7E7"
        android:textSize="16sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/notifiIcon2"
        app:layout_constraintTop_toBottomOf="@+id/notifiText" />
    <TextView
        android:id="@+id/notifi2"
        android:layout_width="407dp"
        android:layout_height="70dp"
        android:background="#007C80"
        android:text="  Post your first feed"
        android:textColor="#F8F0F0"
        android:textSize="16sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/notifiIcon2"
        app:layout_constraintTop_toBottomOf="@+id/notifi1" />

    <TextView
        android:id="@+id/notifi"
        android:layout_width="407dp"
        android:layout_height="70dp"
        android:background="#007C80"
        android:text="First feed added succesfully. Keep in touch"
        android:textColor="#F1ECEC"
        android:textSize="16sp"
        android:textStyle="bold|italic"
        app:layout_constraintTop_toBottomOf="@+id/notifi2"
        tools:layout_editor_absoluteX="49dp"
        tools:ignore="MissingConstraints" />
    <TextView
        android:id="@+id/notifi3"
        android:layout_width="407dp"
        android:layout_height="70dp"
        android:background="#007C80"
        android:text=" 2 Views for your feed"
        android:textColor="#F6F5F5"
        android:textSize="16sp"
        android:textStyle="bold|italic"
        app:layout_constraintTop_toBottomOf="@+id/notifi"
        tools:layout_editor_absoluteX="50dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/notifiIcon2"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="#007C80"
        android:src="@drawable/notification_icon_"
        app:layout_constraintTop_toTopOf="@+id/notifi1"
        tools:layout_editor_absoluteX="-1dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/notifiIcon"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="#007C80"
        android:src="@drawable/notification_icon_"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="178dp"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/notifiIcon3"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="#007C80"
        android:src="@drawable/notification_icon_"
        app:layout_constraintEnd_toStartOf="@+id/notifi"
        app:layout_constraintTop_toBottomOf="@+id/notifiIcon" />

    <ImageView
        android:id="@+id/notifiIcon4"
        android:layout_width="50dp"
        android:layout_height="70dp"
        android:background="#007C80"
        android:src="@drawable/notification_icon_"
        app:layout_constraintTop_toBottomOf="@+id/notifiIcon3"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/NDate1"
        android:layout_width="121dp"
        android:layout_height="39dp"
        android:layout_marginStart="240dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="date|datetime"
        android:text="12th March 2023"
        android:textColor="#F4F1F1"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/notifi1"
        app:layout_constraintTop_toTopOf="@+id/notifi1" />
    <EditText
        android:id="@+id/NDate"
        android:layout_width="121dp"
        android:layout_height="39dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="date|datetime"
        android:text="12th March 2023"
        android:textColor="#FBF7F7"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/notifi2"
        app:layout_constraintTop_toBottomOf="@+id/notifi1" />

    <EditText
        android:id="@+id/NDate3"
        android:layout_width="121dp"
        android:layout_height="39dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="date|datetime"
        android:text="13th March 2023"
        android:textColor="#F6F1F1"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/notifi"
        app:layout_constraintTop_toBottomOf="@+id/notifi2" />
    <EditText
        android:id="@+id/NDate4"
        android:layout_width="121dp"
        android:layout_height="39dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="date|datetime"
        android:text="13th March 2023"
        android:textColor="#FBF8F8"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/notifi3"
        app:layout_constraintTop_toBottomOf="@+id/notifi" />

    <RelativeLayout
        android:layout_width="418dp"
        android:layout_height="74dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:background="#FFFFFF"
        android:padding="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <ImageButton
            android:id="@+id/imageButton21"
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:contentDescription="@string/app_name"
            app:srcCompat="@drawable/ic_baseline_home_24" />
        <ImageButton
            android:id="@+id/imageButton11"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="71dp"
            android:background="#FFFFFF"
            app:srcCompat="@drawable/ic_baseline_support_agent_24" />

        <ImageButton
            android:id="@+id/imageButton20"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="31dp"
            android:layout_marginEnd="196dp"
            android:layout_toEndOf="@+id/imageButton11"
            android:background="#FFFFFF"
            android:contentDescription="@string/app_name"
            android:textAlignment="center"
            app:srcCompat="@drawable/notifications_24" />

        <ImageButton
            android:id="@+id/imageButton22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageButton20"
            android:layout_marginEnd="-77dp"
            android:background="#FFFFFF"
            app:srcCompat="@drawable/ic_baseline_account_circle_24" />
        <ImageButton
            android:id="@+id/imageButton23"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignEnd="@+id/imageButton22"
            android:layout_marginEnd="-90dp"
            android:background="#FFFFFF"
            app:srcCompat="@drawable/ic_baseline_stars_24" />
    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>